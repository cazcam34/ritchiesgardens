      <% end %>
    </div>
    <div class="footer-phone">
      <i class="fas fa-phone-volume"></i>
      <%= link_to "tel: +613 423 418 087", target: :_blank do %>
        <span>0423 418 087</span>
      <% end %>
    </div>
    <div class="socials">
      <%= link_to "http://facebook.com/ritchiesgardens", target: :_blank do %>
